{
	"name": "@brlt/prettier",
	"version": "1.6.2",
	"license": "MIT",
	"author": {
		"name": "Nicholas Berlette",
		"email": "nick@berlette.com",
		"url": "https://berlette.com"
	},
	"packageManager": "pnpm@6.32.3",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"lint": "prettier --check '**/*.{ts,js,json,md,yml,yaml}'",
		"format": "prettier --write '**/*.{ts,js,json,md,yml,yaml}'",
		"preversion": "git commit -a -m 'ðŸ”– prep for release'",
		"version": "pnpm version --no-git-checks --no-commit-hooks --sign-git-tag --force",
		"prepublishOnly": "pnpm format && rm -rf node_modules && pnpm run version patch",
		"release:npm": "pnpm publish --registry=https://registry.npmjs.org",
		"postpublish": "git push -u origin main --follow-tags"
	},
	"description": "Prettier configurations used in my projects.",
	"repository": "nberlette/prettier",
	"keywords": [
		"prettier",
		"config",
		"format"
	],
	"files": [
		"index.js",
		"base.js",
		"bash.js",
		"svelte.js",
		"vue.js"
	],
	"main": "./index.js",
	"types": "./index.d.ts",
	"exports": {
		".": {
			"types": "./index.d.ts",
			"require": "./index.js",
			"import": "./index.js",
			"default": "./index.js"
		},
		"./base": "./base.js",
		"./bash": "./bash.js",
		"./svelte": "./svelte.js",
		"./vue": "./vue.js",
		"./package.json": "./package.json",
		"./*": "./*"
	},
	"devDependencies": {
		"prettier": "2.6.0"
	}
}
