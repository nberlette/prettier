{
	"name": "@brlt/prettier",
	"version": "1.7.2",
	"license": "MIT",
	"author": {
		"name": "Nicholas Berlette",
		"email": "nick@berlette.com",
		"url": "https://github.com/nberlette"
	},
	"packageManager": "pnpm@7.1.0",
	"pnpm": {
		"peerDependencyRules": {
			"ignoreMissing": [
				"svelte"
			]
		}
	},
	"private": false,
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"scripts": {
		"prettier": "prettier --ignore-path=.gitignore --no-error-on-unmatched-pattern",
		"lint": "nr prettier -c '**/*.{js,jsx,ts,tsx}'",
		"format": "nr prettier -w '**/*.{js,jsx,ts,tsx}'",
		"preversion": "nr format && nr lint",
		"version": "git add --all",
		"postversion": "npm publish --registry=https://registry.npmjs.org --no-git-checks --no-commit-hooks --allow-same-version --no-git-tag-version",
		"postpublish": "npx -q -y rimraf node_modules",
		"release:base": "_v(){npx -q -y bumpp ${1:-patch} --tag --commit ${2:-'ðŸ“¦ %s'} package.json pnpm-lock.yaml readme.md index.d.ts {index,all,bash,react,svelte,vue}.js ;};_v $*;"
	},
	"repository": "nberlette/prettier",
	"homepage": "https://npm.im/@brlt/prettier",
	"readme": "https://github.com/nberlette/prettier#readme",
	"bugs": "https://github.com/nberlette/prettier/issues",
	"description": "My personal prettier-config rules for Bash, React, Svelte, and Vue projects.",
	"keywords": [
		"prettier",
		"config",
		"format"
	],
	"files": [
		"index.d.ts",
		"index.js",
		"bash.js",
		"react.js",
		"svelte.js",
		"vue.js"
	],
	"main": "index.js",
	"types": "index.d.ts",
	"exports": {
		".": {
			"types": "./index.d.ts",
			"require": "./index.js",
			"import": "./index.js"
		},
		"./bash": "./bash.js",
		"./react": "./react.js",
		"./svelte": "./svelte.js",
		"./index.js": "./index.js",
		"./base": "./index.js",
		"./vue": "./vue.js",
		"./vue.js": "./vue.js",
		"./bash.js": "./bash.js",
		"./react.js": "./react.js",
		"./svelte.js": "./svelte.js",
		"./index.d.ts": "./index.d.ts",
		"./package.json": "./package.json"
	},
	"devDependencies": {
		"@brlt/n": "0.1.1",
		"@types/prettier": "2.6.1",
		"pnpm": "7.1.0",
		"prettier": "2.6.2"
	},
	"peerDependencies": {
		"prettier": "*"
	}
}
